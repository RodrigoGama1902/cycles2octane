  "ShaderNodeMixShader": {
    "inputs": {
      "0": "0",
      "1": "1",
      "2": "2"
    },

    "outputs": {
      "0": "0"
    },

    "octane_node": "OctaneMixMaterial"
  },

  "ShaderNodeAddShader": {
    "inputs": {
      "0": "1",
      "1": "2"
    },

    "outputs": {
      "0": "0"
    },

    "octane_node": "OctaneMixMaterial"
  },

  "ShaderNodeBump": {
    "inputs": {
      "Strength": "Strength",
      "Distance": "Distance",
      "Height": "Height",
      "Normal": "Normal"
    },

    "outputs": {
      "Normal": "Normal"
    },

    "group_inputs": {
      "Strength": "NodeSocketFloatFactor",
      "Distance": "NodeSocketFloatFactor",
      "Height": "NodeSocketShader",
      "Normal": "NodeSocketShader"
    },
    "group_outputs": {
      "Bump": "NodeSocketShader",
      "Normal": "NodeSocketShader"
    },

    "null_links": {
      "Height": "Bump",
      "Normal": "Normal"
    },

    "octane_node": "None"
  },

  "ShaderNodeNormalMap": {
    "inputs": {
      "Strength": "Strength",
      "Color": "Color"
    },

    "outputs": {
      "Normal": "Normal"
    },

    "group_inputs": {
      "Strength": "NodeSocketFloatFactor",
      "Color": "NodeSocketShader"
    },
    "group_outputs": { "Normal": "NodeSocketShader" },

    "null_links": {
      "Color": "Normal"
    },

    "octane_node": "None"
  },

  "ShaderNodeHueSaturation": {
    "inputs": {
